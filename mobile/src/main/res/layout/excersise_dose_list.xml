<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginRight="5dp"
    android:layout_marginBottom="10dp"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:gravity="center"
    android:orientation="horizontal">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <TextView
            android:id="@+id/hello"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:layout_weight="1"
            android:gravity="center"
            android:textColor="@color/color_white"
            android:textSize="17sp" />

        <RelativeLayout
            android:id="@+id/rLayoutx"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center">


            <TextView
                android:id="@+id/setName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:fontFamily="@font/agencyr"
                android:gravity="center"
                android:text=""
                android:textAlignment="center"
                android:textColor="@color/color_white"
                android:textSize="17sp" />

            <RelativeLayout
                android:id="@+id/Repsvaluex"
                android:layout_width="100dp"
                android:layout_height="35dp"

                android:layout_below="@+id/setName"
                android:layout_centerInParent="true"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"

                android:gravity="center_vertical">

                <EditText
                    android:id="@+id/edittext1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"

                    android:layout_centerInParent="true"
                    android:background="@drawable/bg_dark_rounded"
                    android:digits="0123456789"
                    android:ellipsize="start"
                    android:gravity="center_horizontal|center_vertical"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:maxLines="1"
                    android:textColor="@color/color_white"
                    android:textCursorDrawable="@color/color_white"
                    android:visibility="gone"
                    tools:ignore="EllipsizeMaxLines" />
            </RelativeLayout>

            <EditText
                android:id="@+id/edittext2"
                android:layout_width="120dp"
                android:layout_height="35dp"
                android:layout_below="@+id/Repsvaluex"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_dark_rounded"
                android:digits="0123456789"
                android:gravity="center"
                android:inputType="phone"

                android:maxLines="1"
                android:textColor="@color/color_white"
                android:textCursorDrawable="@color/color_white"
                android:visibility="gone" />

            <EditText
                android:id="@+id/edittext3"
                android:layout_width="120dp"
                android:layout_height="35dp"
                android:layout_below="@+id/edittext2"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_dark_rounded"
                android:digits="0123456789"
                android:gravity="center"
                android:inputType="phone"

                android:maxLines="1"
                android:textColor="@color/color_white"
                android:textCursorDrawable="@color/color_white"
                android:visibility="gone" />

            <EditText
                android:id="@+id/edittext4"
                android:layout_width="120dp"
                android:layout_height="35dp"
                android:layout_below="@+id/edittext3"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_dark_rounded"
                android:digits="0123456789"
                android:gravity="center"
                android:inputType="phone"

                android:maxLines="1"
                android:textColor="@color/color_white"
                android:textCursorDrawable="@color/color_white"
                android:visibility="gone" />

            <EditText
                android:id="@+id/edittext5"
                android:layout_width="120dp"
                android:layout_height="35dp"
                android:layout_below="@+id/edittext4"
                android:layout_marginTop="10dp"
                android:background="@drawable/bg_dark_rounded"
                android:digits="0123456789"
                android:gravity="center"
                android:inputType="phone"

                android:maxLines="1"
                android:textColor="@color/color_white"
                android:textCursorDrawable="@color/color_white"
                android:visibility="gone" />
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/LayoutForHideAdddset"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/hello"
            android:layout_toRightOf="@+id/rLayoutx"
            android:layout_weight="1"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/OpenAddset"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:padding="10dp"
                android:src="@drawable/right_arrow"
                android:tint="@color/textColorYellow"
                android:visibility="gone" />

            <View
                android:id="@+id/forFakemargin"
                android:layout_width="5dp"
                android:layout_height="5dp" />

            <LinearLayout
                android:id="@+id/addsetAthleteExcersiseDose"
                android:layout_width="90dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@drawable/bg_dark_rounded"
                android:gravity="center">

                <ImageView
                    android:id="@+id/video_thumb"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_margin="3dp"
                    android:scaleType="fitXY"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/hhs"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="9dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="8dp"
                    android:gravity="center"
                    android:text="ADD SET"
                    android:textColor="@color/textColorYellow"
                    android:textSize="20sp" />

            </LinearLayout>
        </LinearLayout>

    </RelativeLayout>


</LinearLayout>

